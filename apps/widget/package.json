    "@babel/preset-env": "~7.23.2",
    "@babel/preset-react": "~7.13.13",
    "@babel/preset-typescript": "~7.13.0",
    "@babel/runtime": "~7.20.13",
    "@emotion/babel-plugin": "~11.7.2",
    "@emotion/react": "~11.7.1",
    "@emotion/styled": "~11.6.0",
    "@faker-js/faker": "~9.5.0",
    "@khulnasoft/dal": "~2.0.5",
    "@khulnasoft/testing": "~2.0.5",
    "@types/jest": "~29.5.13",
    "@types/node": "~22.7.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-router-dom": "~5.1.7",
    "cross-env": "~7.0.3",
    "customize-cra": "~1.0.0",
    "cypress": "~13.3.1",
    "cypress-intellij-reporter": "~0.0.7",
    "cypress-network-idle": "~1.14.2",
    "html-webpack-plugin": "~5.5.3",
    "http-server": "~0.13.0",
    "jest": "~29.7.0",
    "less": "~4.1.0",
    "less-loader": "~4.1.0",
    "react-app-rewired": "~2.2.1",
    "react-scripts": "~5.0.1",
    "typescript": "~5.6.2",
    "webpack": "~5.94.0",
    "webpack-dev-server": "~4.11.1"
  },
  "nx": {
    "tags": [
      "type:app"
    ]
  },
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 DISABLE_ESLINT_PLUGIN=true react-app-rewired --max_old_space_size=4096 build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "docker:build": "docker buildx build -f ./Dockerfile --load -t khulnasoft-widget ./../.. $DOCKER_BUILD_ARGUMENTS",
    "docker:build:depot": "depot build -f ./Dockerfile -t khulnasoft-widget ./../.. --load",
    "envsetup": "chmod +x ./env.sh && ./env.sh && mv env-config.js ./public/env-config.js",
    "envsetup:docker": "chmod +x ./env.sh && ./env.sh && mv ./env-config.js ./build/env-config.js",
    "lint": "eslint src",
    "prebuild": "rimraf build",
    "precommit": "lint-staged",
    "start": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "start:api": "cd ../../ && pnpm run start:api:test",
    "start:cli:local": "pnpm start:shell:test & pnpm start:shell:embed & cross-env PORT=3500 pnpm start",
    "start:dev": "cross-env PORT=4500 pnpm start",
    "start:docker": "pnpm build && pnpm start:static:build",
    "start:shell:embed": "http-server ../../libs/embed/dist -p 4702",
    "start:shell:test": "http-server . -p 4700",
    "start:static:build": "pnpm envsetup:docker && http-server build -p 4500 --proxy http://127.0.0.1:4500?",
    "start:test": "pnpm start:shell:test & pnpm start:shell:embed & cross-env NODE_ENV=test REACT_APP_API_URL=http://127.0.0.1:1336 REACT_APP_WS_URL=http://127.0.0.1:1340 PORT=3500 pnpm start"
  },
  "workspaces": {
    "nohoist": [
      "**/@cypress",
      "**/@cypress/**",
      "**/@ant-design/icons",
      "**/@ant-design/icons/**",
      "**/antd",
      "**/antd/**",
      "**/react-scripts",
      "**/react-scripts/**",
      "**/react",
      "**/react-dom",
      "**/react/**",
      "**/react-router",
      "**/react-router/**",
      "**/react-router-dom",
      "**/react-router-dom/**",
      "**/@cypress",
      "**/@cypress/**",
      "**/@storybook/**",
      "**/@storybook",
      "**/@babel",
      "**/@babel/**"
    ]
  }
}
